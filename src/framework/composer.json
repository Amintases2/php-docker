{
    "name": "pfw/framework",
    "minimum-stability": "dev",
    "version": "dev-main",
    "autoload": {
        "psr-4": {
            "PFW\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PFW\\Framework\\Test\\": "tests"
        }
    },
    "scripts": {
        "run-tests": "vendor/bin/phpunit tests --colors"
    },
    "authors": [
        {
            "name": "marat"
        }
    ],
    "require": {
        "nikic/fast-route": "^1.3",
        "league/container": "^4.2.0",
        "symfony/dotenv": "7.2.x-dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2@dev",
        "symfony/var-dumper": "7.2.x-dev"
    }
}
